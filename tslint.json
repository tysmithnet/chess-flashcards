{
    "extends": [
        "tslint:latest", "tslint-react"
    ],
    "rules": {
        "arrow-parens": false,
        "completed-docs": true,
        "jsx-no-multiline-js": false,
        "max-classes-per-file": false,
        "max-line-length": false,
        "no-console": false,
        "no-implicit-dependencies": false,
        "no-submodule-imports": false,
        "no-unused-variable": true,
        "variable-name": false
    }
}